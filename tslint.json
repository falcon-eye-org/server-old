{
    "defaultSeverity": "error",
    "extends": ["tslint-config-security"],
    "jsRules": {},
    "rules": {
        "no-empty-interface": true,
        "prefer-for-of": true,
        "only-arrow-functions": [true, "allow-declarations", "allow-named-functions"],
        "typedef": [true, "call-signature", "parameter"],
        "curly": [true, "as-needed"],
        "no-console": true,
        "no-debugger": true,
        "no-eval": true,
        "no-floating-promises": true,
        "no-invalid-template-strings": true,
        "no-invalid-this": true,
        "no-unused-expression": true,
        "max-classes-per-file": [true, 1],
        "deprecation": true,
        "no-var-keyword": true,
        "restrict-plus-operands": true,
        "prefer-const": true,
        "array-type": [true, "generic"],
        "triple-equals": true,
        "use-isnan": true,
        "await-promise": true,
        "arrow-return-shorthand": true,
        "binary-expression-operand-order": true,
        "class-name": true,
        "no-boolean-literal-compare": true,
        "no-unnecessary-callback-wrapper": true,
        "one-variable-per-declaration": true,
        "no-void-expression": true,
        "static-this": true,
        "no-misused-new": true,
        "prefer-method-signature": true,
        "space-before-function-paren": [true, "never"],
        "variable-name": [true, "ban-keywords", "check-format", "require-const-for-all-caps"],
        "align": [true, "parameters", "arguments", "statements", "members", "elements"],
        "arrow-parens": true,
        "eofline": true,
        "import-spacing": true,
        "indent": [true, "spaces", 4],
        "linebreak-style": [true, "LF"],
        "no-consecutive-blank-lines": true,
        "no-irregular-whitespace": true,
        "no-trailing-whitespace": true,
        "number-literal-format": true,
        "semicolon": [true, "always"],
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
        "whitespace": [true, "check-branch", "check-preblock", "check-operator", "check-decl", "check-module", "check-separator", "check-rest-spread", "check-type", "check-type-operator", "check-typecast"],
        "completed-docs": true
    },
    "rulesDirectory": []
}